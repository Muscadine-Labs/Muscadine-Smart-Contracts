{
  "name": "@muscadine-labs/contracts",
  "version": "2.0.0",
  "description": "Muscadine Labs Smart Contracts",
  "scripts": {
    "build": "hardhat compile",
    "test": "hardhat test",
    "coverage": "hardhat coverage",
    "test:coverage": "hardhat coverage",
    "deploy": "hardhat run contracts/fee-splitter-contracts/ERC20FeeSplitter/scripts/deployImmutable.ts",
    "deploy:base": "hardhat run contracts/fee-splitter-contracts/ERC20FeeSplitter/scripts/deployImmutable.ts --network base",
    "deploy:v2": "hardhat run contracts/fee-splitter-contracts/ERC20FeeSplitter-V2/scripts/deployV2.ts",
    "deploy:v2:base": "hardhat run contracts/fee-splitter-contracts/ERC20FeeSplitter-V2/scripts/deployV2.ts --network base",
    "claim:v1": "hardhat run contracts/fee-splitter-contracts/ERC20FeeSplitter/scripts/claimAllV1.ts",
    "claim:v1:base": "hardhat run contracts/fee-splitter-contracts/ERC20FeeSplitter/scripts/claimAllV1.ts --network base",
    "claim:v2": "hardhat run contracts/fee-splitter-contracts/ERC20FeeSplitter-V2/scripts/claimAllV2.ts",
    "claim:v2:base": "hardhat run contracts/fee-splitter-contracts/ERC20FeeSplitter-V2/scripts/claimAllV2.ts --network base",
    "manage:v2": "hardhat run contracts/fee-splitter-contracts/ERC20FeeSplitter-V2/scripts/managePayees.ts",
    "manage:v2:base": "hardhat run contracts/fee-splitter-contracts/ERC20FeeSplitter-V2/scripts/managePayees.ts --network base",
    "tokens:v2": "hardhat run contracts/fee-splitter-contracts/ERC20FeeSplitter-V2/scripts/manageClaimableTokens.ts",
    "tokens:v2:base": "hardhat run contracts/fee-splitter-contracts/ERC20FeeSplitter-V2/scripts/manageClaimableTokens.ts --network base",
    "transfer-owner:v2": "hardhat run contracts/fee-splitter-contracts/ERC20FeeSplitter-V2/scripts/transferOwnership.ts",
    "transfer-owner:v2:base": "hardhat run contracts/fee-splitter-contracts/ERC20FeeSplitter-V2/scripts/transferOwnership.ts --network base",
    "lint": "solhint 'contracts/**/*.sol'",
    "lint:fix": "solhint 'contracts/**/*.sol' --fix",
    "format": "prettier --write 'contracts/**/*.sol' 'contracts/**/test/**/*.ts' 'contracts/**/scripts/**/*.ts'",
    "format:check": "prettier --check 'contracts/**/*.sol' 'contracts/**/test/**/*.ts' 'contracts/**/scripts/**/*.ts'"
  },
  "keywords": [
    "ethereum",
    "solidity"
    
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.1.0",
    "@nomicfoundation/hardhat-ethers": "^3.1.2",
    "@nomicfoundation/hardhat-ignition": "^0.15.15",
    "@nomicfoundation/hardhat-ignition-ethers": "^0.15.16",
    "@nomicfoundation/hardhat-network-helpers": "^1.1.2",
    "@nomicfoundation/hardhat-toolbox": "^6.1.2",
    "@nomicfoundation/hardhat-verify": "^2.1.3",
    "@nomicfoundation/ignition-core": "^0.15.14",
    "@openzeppelin/contracts": "^5.4.0",
    "@openzeppelin/contracts-upgradeable": "^5.4.0",
    "@openzeppelin/hardhat-upgrades": "^3.9.1",
    "@typechain/ethers-v6": "^0.5.1",
    "@typechain/hardhat": "^9.1.0",
    "@types/chai": "^4.3.20",
    "@types/mocha": "^10.0.10",
    "@types/node": "^25.0.3",
    "chai": "^4.5.0",
    "dotenv": "^17.2.3",
    "ethers": "^6.16.0",
    "hardhat": "^2.28.2",
    "hardhat-gas-reporter": "^2.3.0",
    "prettier": "^3.7.4",
    "prettier-plugin-solidity": "^2.2.1",
    "solhint": "^6.0.2",
    "solidity-coverage": "^0.8.17",
    "ts-node": "^10.9.2",
    "typechain": "^8.3.2",
    "typescript": "^5.9.3"
  }
}
